<?xml version="1.0"?>
<global>
	<settings>
		<LineNumber>true</LineNumber>
		<Fold>true</Fold>
		<setting name="global" style-iden=0>
			<BackColor>#FFFFFF</BackColor>
			<ForeColor>#000000</ForeColor>
			<FontName>Courier</FontName>
			<FontSize>10</FontSize>
			<FontItalic>false</FontItalic>
			<FontBold>false</FontBold>
		</setting>
	</settings>
</global>


<languages>
	<language name="C" file="*.c">
		<KeyWords>asm auto break
				case catch class const continue
				default do else enum export extern false for
				goto if inline register return static struct switch
				union volatile while
		</KeyWords>
		<ExternalOperators>sizeof typeof typedef
		</ExternalOperators>
		<BuiltinTypes>float int long short signed unsigned void wchar_t char double
		</BuiltinTypes>
		<settings>
			<Default name='default' style-iden=0>
				<FontSize>10</FontSize>
